{
  "name": "vue3-google-map",
  "version": "0.23.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inocan-group/vue3-google-maps.git"
  },
  "homepage": "https://vue3-google-map.com",
  "description": "A set of composable components for easy use of Google Maps in your Vue 3 projects.",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./themes": {
      "import": "./dist/themes/index.mjs",
      "require": "./dist/themes/index.cjs",
      "types": "./dist/types/themes/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ],
      "themes": [
        "dist/types/themes/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "prepare": "pnpm run build",
    "clean": "rimraf dist/**/*",
    "lint": "npx eslint --ext .ts,.vue src playground --fix",
    "lint:check": "npx eslint --ext .ts,.vue src playground",
    "build:umd": "node ./scripts/build-umd.js",
    "build": "pnpm clean && vue-tsc --declaration --emitDeclarationOnly --project tsconfig.build.json && vite build && pnpm build:umd",
    "docs": "vitepress dev docs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.2",
    "@googlemaps/markerclusterer": "^2.4.0",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@googlemaps/jest-mocks": "^2.22.6",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "pnpm": "^10.13.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.1.6",
    "vite": "^4.5.3",
    "vite-plugin-css-injected-by-js": "^3.2.1",
    "vitepress": "^1.0.0-rc.40",
    "vue": "^3.3.4",
    "vue-tsc": "^3.0.3"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "engines": {
    "node": ">=18.12.0"
  }
}
